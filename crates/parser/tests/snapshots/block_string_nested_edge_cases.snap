SOURCE_FILE@0..34
  SPACE@0..2 "  "
  ENTRY@2..20
    WORD_KEY@2..5
      WORD@2..5 "key"
    SPACE@5..6 " "
    BLOCK_STRING@6..19
      BLOCK_STRING_HEADER@6..8
        PIPE@6..7 "|"
        NEWLINE@7..8 "\n"
      SPACE@8..12 "    "
