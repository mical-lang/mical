SOURCE_FILE@0..27
  ENTRY@0..27
    WORD_KEY@0..3
      WORD@0..3 "key"
    SPACE@3..4 " "
    BLOCK_STRING@4..26
      BLOCK_STRING_HEADER@4..6
        PIPE@4..5 "|"
        NEWLINE@5..6 "\n"
      SPACE@6..8 "  "
      LINE_STRING@8..16
        STRING@8..16 "hello   "
      NEWLINE@16..17 "\n"
